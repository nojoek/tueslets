<h1>Listing events</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Loc1</th>
    <th>Loc2</th>
    <th>Loc3</th>
    <th>Loc4</th>
    <th>Time1</th>
    <th>Time2</th>
    <th>Time3</th>
    <th>Time4</th>
    <th>Description</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @events.each do |event| %>
  <tr>
    <td><%= event.title %></td>
    <td><%= event.loc1 %></td>
    <td><%= event.loc2 %></td>
    <td><%= event.loc3 %></td>
    <td><%= event.loc4 %></td>
    <td><%= event.time1 %></td>
    <td><%= event.time2 %></td>
    <td><%= event.time3 %></td>
    <td><%= event.time4 %></td>
    <td><%= event.description %></td>
    <td><%= link_to 'Show', event %></td>
    <td><%= link_to 'Edit', edit_event_path(event) %></td>
    <td><%= link_to 'Destroy', event, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Event', new_event_path %>
